#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n; 
    scanf("%d",&n);
    int i,j;
    for(i=1;i<=10;i++)
    {
        j=i*n; 
     printf("%d x %d = %d\n",n,i,j);
    }

    
    return 0;
}
